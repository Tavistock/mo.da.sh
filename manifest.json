{
  "manifest_version": 2,

  "name": "modash",
  "description": "This extension is a dash.",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
   },

   "background": {
    "persistent": false,
    "scripts": [ "modash.js" ]
   },

   "offline_enabled": true,

  "permissions": [
    "geolocation",
    "unlimitedStorage",
    "*://*.yahooapis.com/*",
    "bookmarks"
  ],

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://query.yahooapis.com 'unsafe-eval'; object-src 'self'"
}
